{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "es6": true,
        "browser": true,
        "jest": true
    },
    "extends": [
        "airbnb"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    }
}
